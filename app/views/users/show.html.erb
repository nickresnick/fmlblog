<% provide(:title, @user.name) %>
<div class="row">
      <h1><%= @user.name %></h1>
  <div class="col-md-12">
    <% if @user.posts.any? %>
        <h3>Posts (<%= @user.posts.count %>)</h3>
        <hr>
        <ol class="posts">
          <%= render 'shared/feed' %>
        </ol>
       <%# <%= will_paginate @posts %>
    <% end %>
  </div>
  <h4><%= link_to 'Back', url_for(:back) %></h4>
</div>
