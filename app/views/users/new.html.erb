<% provide(:title, 'Sign Up or Shut Up') %>
<h1>Admin Sign-In</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
        <%= render 'shared/fields', f: f %>
        <%= f.submit "Join the party bro!", class: "btn btn-primary" %>
        <% end %>
  </div>
</div>
